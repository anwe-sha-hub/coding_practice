class Solution {
  public:
    bool twoSum(vector<int>& arr, int target) {
        // code here
        unordered_map<int,int>m;
        vector<int>ans;
        for(int i=0;i<arr.size();i++){
            int first=arr[i];
            int sec=target-first;
            if(m.find(sec)!=m.end()){
                return true;
            }
            m[first]=i;
        }
        return false;
    }
};